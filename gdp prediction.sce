function leastsquare = ls(year,gdp)
    a = sum(gdp)/size(gdp,2)
    disp("intercept of line", a)
    meanx = sum(year)/size(year,2)
    x = year - meanx
    xs = x.^2
    xy = gdp.*x
    b = sum(xy)/sum(xs)
    disp("slope of line", b)
    leastsquare = zeros(size(gdp,2))
    for i = 1:size(gdp,2)
        leastsquare(i) = a + b*x(1,i)
    end
endfunction
